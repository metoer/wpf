<UserControl x:Class="Hytera.EEMS.Manage.UserControls.BoardImageControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300" Name="userControl1">
    <Grid>
        <Canvas Background="Transparent" Name="touchPad"  Width="{Binding ElementName=userControl1,Path=ActualWidth}" Height="{Binding ElementName=userControl1,Path=ActualHeight}">

        </Canvas>
    </Grid>
</UserControl>
